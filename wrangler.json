{
	"compatibility_date": "2025-10-08",
	"main": "src/server/index.ts",
	"name": "auto-wol",
	"migrations": [
		{
			"new_sqlite_classes": ["WolManager"],
			"tag": "v1"
		}
	],
	"assets": {
		"directory": "./public",
		"binding": "ASSETS",
		"not_found_handling": "single-page-application"
	},
	"build": {
		"command": "pnpm build"
	},
	"durable_objects": {
		"bindings": [
			{
				"class_name": "WolManager",
				"name": "WolManager"
			}
		]
	},
	"observability": {
		"enabled": true
	},
	"upload_source_maps": true
}
