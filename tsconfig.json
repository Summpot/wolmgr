{
	"compilerOptions": {
		"target": "esnext",
		"lib": ["esnext", "dom", "dom.iterable"],
		"module": "esnext",
		"moduleResolution": "bundler",
		"types": ["@cloudflare/workers-types", "node"],
		"noEmit": true,
		"isolatedModules": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"strict": true
	},
	"include": ["src/**/*.ts", "src/**/*.tsx"]
}
